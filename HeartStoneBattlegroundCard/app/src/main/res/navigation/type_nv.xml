            app:destination="@id/piratesFragment" />
        <action
            android:id="@+id/action_elementFragment_to_dragonFragment"
            app:destination="@id/dragonFragment" />
        <action
            android:id="@+id/action_elementFragment_to_devilFragment"
            app:destination="@id/devilFragment" />
        <action
            android:id="@+id/action_elementFragment_to_beastFragment"
            app:destination="@id/beastFragment" />
    </fragment>
    <fragment
        android:id="@+id/mechaFragment"
        android:name="com.yangsooplus.heartstonebattlegroundcard.MechaFragment"
