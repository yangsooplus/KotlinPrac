<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/type_nv"
    app:startDestination="@id/beastFragment">

    <fragment
        android:id="@+id/beastFragment"
        android:name="com.yangsooplus.heartstonebattlegroundcard.BeastFragment"
        android:label="fragment_beast"
        tools:layout="@layout/fragment_beast" >
        <action
            android:id="@+id/action_beastFragment_to_elementFragment"
            app:destination="@id/elementFragment" />
        <action
            android:id="@+id/action_beastFragment_to_mechaFragment"
            app:destination="@id/mechaFragment" />
        <action
            android:id="@+id/action_beastFragment_to_piratesFragment"
            app:destination="@id/piratesFragment" />
        <action
            android:id="@+id/action_beastFragment_to_dragonFragment"
            app:destination="@id/dragonFragment" />
        <action
            android:id="@+id/action_beastFragment_to_devilFragment"
            app:destination="@id/devilFragment" />
    </fragment>
    <fragment
        android:id="@+id/devilFragment"
        android:name="com.yangsooplus.heartstonebattlegroundcard.DevilFragment"
        android:label="fragment_devil"
        tools:layout="@layout/fragment_devil" >
        <action
            android:id="@+id/action_devilFragment_to_beastFragment"
            app:destination="@id/beastFragment" />
        <action
            android:id="@+id/action_devilFragment_to_elementFragment"
            app:destination="@id/elementFragment" />
