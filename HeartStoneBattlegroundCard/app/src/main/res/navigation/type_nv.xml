            android:id="@+id/action_dragonFragment_to_beastFragment"
            app:destination="@id/beastFragment" />
        <action
            android:id="@+id/action_dragonFragment_to_elementFragment"
            app:destination="@id/elementFragment" />
        <action
            android:id="@+id/action_dragonFragment_to_mechaFragment"
            app:destination="@id/mechaFragment" />
